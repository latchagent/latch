{
  "name": "@latchagent/shared",
  "version": "0.1.0",
  "description": "Shared types and utilities for Latch",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/latchagent/latch.git",
    "directory": "packages/shared"
  },
  "bugs": {
    "url": "https://github.com/latchagent/latch/issues"
  },
  "homepage": "https://github.com/latchagent/latch#readme",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./hash": {
      "types": "./dist/hash.d.ts",
      "default": "./dist/hash.js"
    },
    "./classifier": {
      "types": "./dist/classifier.d.ts",
      "default": "./dist/classifier.js"
    },
    "./redactor": {
      "types": "./dist/redactor.d.ts",
      "default": "./dist/redactor.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "typescript": "^5"
  }
}
